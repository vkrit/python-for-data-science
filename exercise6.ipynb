    "def threetimestotal(numbers):\n",
    "\n",
    "    sum_so_far = 0\n",
    "\n",
    "    for number in numbers:\n",
    "        sum_so_far += (number*3)\n",
    "\n",
    "    return sum_so_far\n",
    "\n",
    "print(threetimestotal([1,2,3]))"
   ]
